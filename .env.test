# テスト環境用の環境変数
NODE_ENV=test

# データベース
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/diary_test_db
DB_HOST=localhost
DB_PORT=5432
DB_NAME=diary_test_db
DB_USER=postgres
DB_PASSWORD=postgres

# Redis
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379

# JWT
JWT_SECRET=test_jwt_secret_key_for_testing_only
JWT_EXPIRES_IN=24h

# OpenAI (テスト用モックキー)
OPENAI_API_KEY=sk-test-mock-key-for-testing

# サーバー
PORT=3001
API_URL=http://localhost:3001

# ログレベル
LOG_LEVEL=error

# その他
ENABLE_RATE_LIMIT=false
ENABLE_CORS=true
